{
  "name": "event-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "deploy": "npm run build && aws s3 sync dist/ s3://ngocquanwd.com --delete && aws cloudfront create-invalidation --distribution-id EJETB6DK025YT --paths '/*'",
    "deploy:sdk": "npm run build && tsx scripts/s3-sync.ts upload",
    "download:sdk": "tsx scripts/s3-sync.ts download",
    "invalidate": "aws cloudfront create-invalidation --distribution-id EJETB6DK025YT --paths '/*'"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.901.0",
    "@aws-sdk/lib-storage": "^3.901.0",
    "@hookform/resolvers": "^5.2.2",
    "@types/qrcode": "^1.5.5",
    "gsap": "^3.13.0",
    "lucide-react": "^0.545.0",
    "qrcode": "^1.5.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.64.0",
    "react-router-dom": "^6.30.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@gsap/react": "^2.1.2",
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.3.0",
    "@studio-freight/lenis": "^1.0.42",
    "@types/node": "^24.6.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/three": "^0.180.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vitejs/plugin-react": "^5.0.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "three": "^0.180.0",
    "tsx": "^4.20.6",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7"
  }
